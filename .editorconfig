# EditorConfig para consistencia entre editores
root = true

[*]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.{js,jsx,ts,tsx}]
max_line_length = 80

[*.md]
trim_trailing_whitespace = false

[package.json]
indent_size = 2